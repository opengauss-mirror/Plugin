                            parser_errposition(@1)));
                    $$ = NULL;/* not reached */
                }

            }
            | REPLACE hint_string replace_empty insert_target SET set_clause_list
            {
#ifndef ENABLE_MULTIPLE_NODES
                if (u_sess->attr.attr_sql.sql_compatibility == B_FORMAT)
                {
                     InsertStmt* n = makeNode(InsertStmt);
                     n->relation = $4;
                     n->targetList = $6;
