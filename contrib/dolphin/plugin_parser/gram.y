				if (NULL == typtup) {
					n->funcname = SystemFuncName("convert");
					TypeName * tmp = SystemTypeName("name");
					tmp->location = @5;
					n->args = list_make2($3, makeStringConstCast(strVal(linitial(typname->names)), -1, tmp));
					n->agg_order = NIL;
					n->agg_star = FALSE;
					n->agg_distinct = FALSE;
