            {
                AlterAuditPolicyStmt *n = makeNode(AlterAuditPolicyStmt);
                n->missing_ok = true;
                n->policy_name = $6;
                n->policy_action = "drop_filter";
                $$ = (Node *) n;
            }

        |
        /* alter policy status */
        ALTER AUDIT POLICY policy_name policy_status_alter_clause
                {
                    AlterAuditPolicyStmt *n = makeNode(AlterAuditPolicyStmt);
                    n->policy_name = $4;
                    n->policy_action = "add";
                    n->policy_comments = "";
                    n->policy_enabled = (Node *)$5;
                    $$ = (Node *) n;
