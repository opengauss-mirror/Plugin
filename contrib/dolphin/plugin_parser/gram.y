					FuncCall *n = makeNode(FuncCall);
					Node *d = makeStringConstCast("now", -1, SystemTypeName("text"));

					n->funcname = SystemFuncName("text_date");
					n->colname = pstrdup("date");
					n->args = list_make1(d);
					n->agg_order = NIL;
					n->agg_star = false;
					n->agg_distinct = false;
					n->func_variadic = false;
					n->over = NULL;
					n->location = @1;
					n->call_func = false;
					$$ = (Node *)n;
