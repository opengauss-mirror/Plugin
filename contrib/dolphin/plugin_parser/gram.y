        ALTER AUDIT POLICY IF_P EXISTS policy_name policy_status_alter_clause
                {
                    AlterAuditPolicyStmt *n = makeNode(AlterAuditPolicyStmt);
                    n->missing_ok = true;
                    n->policy_name = $6;
                    n->policy_action = "add";
                    n->policy_comments = "";
                    n->policy_enabled = (Node *)$7;
                    $$ = (Node *) n;
                }

        ;


