create schema db_optimize;
set current_schema to 'db_optimize';
create table doc(id serial primary key, content varchar(255));
NOTICE:  CREATE TABLE will create implicit sequence "doc_id_seq" for serial column "doc.id"
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "doc_pkey" for table "doc"
insert into doc(content) select 'abcd1234' from generate_series(1,10000) as content;
delete from doc where id < 9000;
optimize no_write_to_binlog table doc;
drop table doc;
set xc_maintenance_mode = on;
optimize table pg_class;
set xc_maintenance_mode = off;
drop schema db_optimize cascade;
reset current_schema;
