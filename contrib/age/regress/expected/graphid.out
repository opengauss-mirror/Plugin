LOAD 'age';
SET search_path TO ag_catalog;
-- basic comparisons
SELECT '0'::graphid = '0'::graphid, '0'::graphid = '1'::graphid;
 ?column? | ?column? 
----------+----------
 t        | f
(1 row)

SELECT '0'::graphid <> '0'::graphid, '0'::graphid <> '1'::graphid;
 ?column? | ?column? 
----------+----------
 f        | t
(1 row)

SELECT '0'::graphid < '1'::graphid,
       '0'::graphid < '0'::graphid,
       '1'::graphid < '0'::graphid;
 ?column? | ?column? | ?column? 
----------+----------+----------
 t        | f        | f
(1 row)

SELECT '0'::graphid > '1'::graphid,
       '0'::graphid > '0'::graphid,
       '1'::graphid > '0'::graphid;
 ?column? | ?column? | ?column? 
----------+----------+----------
 f        | f        | t
(1 row)

SELECT '0'::graphid <= '1'::graphid,
       '0'::graphid <= '0'::graphid,
       '1'::graphid <= '0'::graphid;
 ?column? | ?column? | ?column? 
----------+----------+----------
 t        | t        | f
(1 row)

SELECT '0'::graphid >= '1'::graphid,
       '0'::graphid >= '0'::graphid,
       '1'::graphid >= '0'::graphid;
 ?column? | ?column? | ?column? 
----------+----------+----------
 f        | t        | t
(1 row)

-- b-tree index
CREATE TABLE graphid_table (gid graphid);
INSERT INTO graphid_table VALUES ('0'), ('1'), ('2');
CREATE INDEX ON graphid_table (gid);
SET enable_seqscan = OFF;
EXPLAIN (COSTS FALSE) SELECT * FROM graphid_table WHERE gid = '1';
                          QUERY PLAN                          
--------------------------------------------------------------
 Index Only Scan using graphid_table_gid_idx on graphid_table
   Index Cond: (gid = '1'::graphid)
(2 rows)

EXPLAIN (COSTS FALSE) SELECT * FROM graphid_table WHERE gid > '0';
                          QUERY PLAN                          
--------------------------------------------------------------
 Index Only Scan using graphid_table_gid_idx on graphid_table
   Index Cond: (gid > '0'::graphid)
(2 rows)

SET enable_seqscan = ON;
DROP TABLE graphid_table;
